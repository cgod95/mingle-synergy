# .env.example - Copy to .env.development or .env.local and fill in values

# Firebase Configuration (Required)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Firebase Emulators (Development Only)
VITE_FIREBASE_AUTH_EMULATOR_HOST=http://localhost:9099
VITE_FIREBASE_FIRESTORE_EMULATOR_HOST=8080

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true
VITE_USE_MOCK=false

# Services
VITE_API_BASE_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:8080

# Payment Processing (Optional)
VITE_STRIPE_PUBLISHABLE_KEY=

# Push Notifications (Optional)
VITE_VAPID_PUBLIC_KEY=

# Monitoring (Optional)
VITE_SENTRY_DSN=
VITE_ENVIRONMENT=development

# Microservices (Optional)
VITE_AUTH_SERVICE_URL=http://localhost:3001
VITE_USER_SERVICE_URL=http://localhost:3002
VITE_MATCHING_SERVICE_URL=http://localhost:3003
VITE_MESSAGING_SERVICE_URL=http://localhost:3004
